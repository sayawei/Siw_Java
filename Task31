public class Task31 {
    public static void main(String[] args) {
        int num1 = 10;
        int num2 = 5;

        int sum = num1 + num2;
        int difference = num1 - num2;

        System.out.println("Sum: " + sum);
        System.out.println("Difference: " + difference);
    }
}

//Output: Sum: 15 Difference: 5
