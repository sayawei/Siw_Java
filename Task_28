public class Task_28 {
    public static void main(String[] args) {
        int[] array = {7, 2, 9, 4, 1, 6};

        int min = array[0];
        
        for (int i = 1; i < array.length; i++) {
            if (array[i] < min) {
                min = array[i];
            }
        }

        System.out.println("Minimum element in the array: " + min);
    }
}

//Output: Minimum element in the array: 1
